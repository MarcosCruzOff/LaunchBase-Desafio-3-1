{% extends "layout.njk" %}
{% block content %}
   <div class="cards">
      {% for item in items %}
         <div class="card" id="{{ item.id }}" style='background-color:{{ item.corFundo }}'>
            <img src="{{ item.imgCurso }}" alt="{{ item.titulo }}"/>
            <h1>{{ item.titulo }}</h1>
         </div>
      {% endfor %}
   </div>
{% endblock content %}
{% block script %}
   <script src="../../index.js"></script>
{% endblock script %}